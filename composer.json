{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-zip": "*",
        "antonrom00/laravel-model-changes-history": "dev-master",
        "barryvdh/laravel-dompdf": "^2.0",
        "beyondcode/laravel-websockets": "^1.13.1",
        "bjeavons/zxcvbn-php": "^1.3.1",
        "dive-be/laravel-dry-requests": "^2.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^v0.6.4",
        "intervention/image": "^2.7",
        "laravel/framework": "^v9.40.1",
        "laravel/jetstream": "^v2.12.5",
        "laravel/sanctum": "^v2.15.1",
        "laravel/scout": "^v9.4.12",
        "laravel/tinker": "^v2.7.3",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^v0.23.3",
        "pestphp/pest": "^v1.22.2",
        "pestphp/pest-plugin-laravel": "^v1.3.0",
        "pusher/pusher-php-server": "7.0.2",
        "spatie/laravel-permission": "^5.5.16",
        "spatie/laravel-settings": "^2.5.0",
        "symfony/http-client": "^v6.1.7",
        "symfony/mailgun-mailer": "^v6.1.0",
        "symfony/event-dispatcher": "^v6.1.0",
        "tightenco/ziggy": "^v1.5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-debugbar": "^v3.7.0",
        "barryvdh/laravel-ide-helper": "^v2.12.3",
        "fakerphp/faker": "^v1.20.0",
        "laravel/sail": "^v1.16.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^v6.3.1",
        "pestphp/pest-plugin-faker": "^1.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5.26",
        "slevomat/coding-standard": "^8.14",
        "spatie/laravel-ignition": "^1.6.1",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Artwork\\": "artwork/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": [
            "php vendor/bin/phpstan analyse ./app ./artwork ./config ./database ./routes --memory-limit='1G'"
        ],
        "phpcs": [
            "php vendor/bin/phpcs"
        ],
        "phpcbf": [
            "php vendor/bin/phpcbf"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
